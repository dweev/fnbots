{
  "name": "fnbots",
  "version": "3.1.0",
  "description": "FNBOTS FN WHATSAPP BOT",
  "main": "core/main.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Terror-Machine/fnbots.git"
  },
  "bugs": {
    "url": "https://github.com/Terror-Machine/fnbots/issues"
  },
  "homepage": "https://github.com/Terror-Machine/fnbots#readme",
  "scripts": {
    "start": "node --expose-gc --max-old-space-size=4096 core/main.js",
    "dev": "nodemon core/main.js --ignore node_modules --ignore logs --ignore src/sampah --expose-gc --max-old-space-size=4096",
    "pair": "node core/main.js --pairing-code --expose-gc --max-old-space-size=4096",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --runInBand",
    "build:addon": "node-gyp configure && node-gyp build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "format": "prettier --write ."
  },
  "keywords": [
    "whatsapp",
    "bot",
    "baileys"
  ],
  "author": "Terror-Machine",
  "license": "MIT",
