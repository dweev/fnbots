    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "arrowParens": "always",
    "bracketSameLine": true,
    "quoteProps": "preserve",
    "overrides": []
  }
